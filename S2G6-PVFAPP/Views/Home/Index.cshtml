<style>
    .carousel-caption {
        position: absolute;
        right: 15%;
        bottom: 141px;
        left: 15%;
        z-index: 10;
        padding-top: 20px;
        padding-bottom: 20px;
        color: #fff;
        text-align: center;
        text-shadow: 0 1px 2px rgba(0, 0, 0, 0.6);
    }

    h1 {
        font-size: 57px;
    }
    h3{
             background-color: #ef7350;
    width: 300px;
    border: 15px crimson;
    padding: 10px;
    margin-left: -870px;
    margin-top: -48px;
  border-top-left-radius: 8px;
    }
   
</style>
<div>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <h1 class="carousel-caption h1size">Pine Valley Furniture Company</h1>

        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="https://i.ibb.co/Mhc7djD/chair3.jpg" alt="Los Angeles" style="width:100%; height: 400px">
            </div>

            <div class="item">
                <img src="https://i.ibb.co/4gWdMbY/sofa.jpg" alt="Chicago" style="width:100%;height: 400px">
            </div>

            <div class="item">
                <img src="https://i.ibb.co/jzmTqDZ/table3.jpg" alt="New york" style="width:100%;height: 400px">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>

